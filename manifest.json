{
  "manifest_version": 2,
  "name": "codes",
  "description": "代码片段管理",
  "icons":{"128":"icon.png"},
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://git.oschina.net; object-src 'self'",
  "permissions": [
    "activeTab",
    "http://localhost:8080/",
    "https://git.oschina.net/"
  ]
}